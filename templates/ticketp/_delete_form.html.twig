<form method="post" action="{{ path('app_ticketp_delete', {'ticketpId': ticketp.ticketpId}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ ticketp.ticketpId) }}">
    <button class="btn">Delete</button>
</form>
