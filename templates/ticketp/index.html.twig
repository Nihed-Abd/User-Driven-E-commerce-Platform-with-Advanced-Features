{% extends 'base.html.twig' %}

{% block title %}Ticketp index{% endblock %}

{% block body %}
    <h1>Ticketp index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>TicketpId</th>
                <th>TicketId</th>
                <th>ClientId</th>
                <th>EnchereId</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for ticketp in ticketps %}
            <tr>
                <td>{{ ticketp.ticketpId }}</td>
                <td>{{ ticketp.ticketId }}</td>
                <td>{{ ticketp.clientId }}</td>
                <td>{{ ticketp.enchereId }}</td>
                <td>
                    <a href="{{ path('app_ticketp_show', {'ticketpId': ticketp.ticketpId}) }}">show</a>
                    <a href="{{ path('app_ticketp_edit', {'ticketpId': ticketp.ticketpId}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_ticketp_new') }}">Create new</a>
{% endblock %}
